<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mehealth">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activities.SettingsActivity"
            android:label="@string/title_activity_asetukset">
        </activity>
        <activity
            android:name=".Activities.MainActivity"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.WeightActivity" />
        <activity android:name=".Activities.WaterActivity"/>
        <activity android:name=".Activities.ExerciseActivity" />
        <activity android:name=".Activities.MoodActivity" />
    </application>

</manifest>